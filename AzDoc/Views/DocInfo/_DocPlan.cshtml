@model IEnumerable<BLL.Models.DocInfo.DocPlan>

@if (Model?.Count() > 0)
{
    <div class="alert alert-light alert-dismissible col-5" style="font-size: smaller" id="alertOperationHistory">

        <table class="table table-bordered">
            <thead>
                <tr>
                    <th scope="col">№</th>
                    <th scope="col">Kimə</th>
                    <th scope="col">Əməliyyatın növü</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.OrderIndex</td>
                        <td>@item.PersonTo</td>
                        <td>@item.ExecutorMain</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
}
